<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<web-app>
    <servlet>
        <servlet-name>login</servlet-name>
        <servlet-class>com.han.oa.servlet.LoginServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>login</servlet-name>
        <url-pattern>/login</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>userSave</servlet-name>
        <servlet-class>com.han.oa.servlet.UserSaveServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>userSave</servlet-name>
        <url-pattern>/user/save</url-pattern>
    </servlet-mapping>
</web-app>